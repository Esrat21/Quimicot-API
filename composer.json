{
    "name": "yanpegyn/chemical-api",
    "description": "API feita em Slim Framework, com objetivo de extrair informações sobre quimica dos jogos",
    "license": "MIT",
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.1",
        "php-di/slim-bridge": "^3.0",
        "rbdwllr/psr-jwt": "^1.0",
        "guzzlehttp/guzzle": "^7.4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5"
    },
    "scripts": {
        "dev": "php -S localhost:80 ./public_html/index.php",
        "test": "./vendor/bin/phpunit --verbose >> ./testes.log",
        "test:prepare": "./vendor/bin/phpunit --verbose ./tests/independente/DB_Prepare_Test.php >> ./testes.log"
    },
    "config": {
        "process-timeout": 0
    }
}